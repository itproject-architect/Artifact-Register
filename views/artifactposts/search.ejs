<% include ../partials/header %>

<section class="searchblogpostpage">
    <div class="container">
        <div class="row">
            <table class="table">
                <caption>Search Results</caption>
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Author</th>
                    </tr>
                </thead>
                <tbody>
                    <% results.forEach(function (artipost) { %>
                        <tr>
                            <td><%= artipost.name %></td>
                            <td><%= artipost.author.username %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</section>

<% include ../partials/footer %>